function BotMessage(Message)
if _G.SendMessages == true then
game:GetService("ReplicatedStorage").Events.PlayerChatted:FireServer(tostring(Message), false, false, false)
else
_G.SendMessages = false
end
end


function ChangeMusic(ID)
if _G.ChangeMusic == true then
game:GetService("ReplicatedStorage").Events.UpdateLoadout:FireServer({"UpdateID", tostring(ID)})
wait(.5)
game:GetService("ReplicatedStorage").Events.UpdateLoadout:FireServer({"UpdateID", tostring(ID)})
else
_G.ChangeMusic = false
end
end


function PlayMusic()
if _G.PlayMusic == true then
local GlitteredNeko =
{
    [1] = "createparticle", 
    [2] = "Blood", 
    [3] = game:GetService("ReplicatedStorage").Pilots.AcePilot.Humanoid, 
    [4] = Vector3.new(255, 255, 255), 
    [5] = Vector3.new(255, 255, 255), 
    [6] = game:GetService("ReplicatedStorage").Weapons.M40, 
    [7] = false, 
    [8] = false, 
    [9] = true, 
    [10] = game:GetService("Players").LocalPlayer.Data.RadioID
}
game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer(GlitteredNeko)
else
_G.PlayMusic = false
end
end
